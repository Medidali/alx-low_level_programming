#include "main.h"
/**
 * _strcat - fuction wich concatainate two string
 * @dest: parametre 1 passed to funct
 * @src:parametre 2 passed to funct 
 *@n: parametre 3 passed to funct 
 *
 * Return:char
 */






char *_strcat(char *dest, char *src, int n)
{
	char *s = dest;

	while (*dest != '\0')
	{
		dest++;
	}
int i = 0;
	while (i != n)
	{
		*dest = *src;
		dest++;
		src++;
		i++;
	}
	*dest = '\0';
	return (s);
}
